{
  "name": "aifeed-platform",
  "version": "1.0.0",
  "description": "AI Intelligence Dashboard - Desktop Application",
  "main": "build/main/main.js",
  "homepage": "./",
  "author": "Jasonn Michaels <sanchez314c@jasonpaulmichaels.co>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:5173 && npm run dev:main\"",
    "dev:renderer": "vite",
    "dev:main": "cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "start": "cross-env NODE_ENV=production electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "dist": "npm run build && electron-builder --mac --win --linux",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "dist:linux": "npm run build && electron-builder --linux",
    "prepare": "echo 'Skipping husky install for build'",
    "dist:current": "npm run build && electron-builder",
    "dist:win:msi": "npm run build && electron-builder --win --config.win.target=msi",
    "dist:win:nsis": "npm run build && electron-builder --win --config.win.target=nsis",
    "dist:linux:appimage": "npm run build && electron-builder --linux --config.linux.target=AppImage",
    "dist:linux:deb": "npm run build && electron-builder --linux --config.linux.target=deb",
    "dist:linux:rpm": "npm run build && electron-builder --linux --config.linux.target=rpm",
    "dist:linux:snap": "npm run build && electron-builder --linux --config.linux.target=snap",
    "dist:maximum": "npm run build && electron-builder --mac --win --linux --x64 --arm64 && electron-builder --win --ia32 && electron-builder --linux --ia32 && electron-builder --linux --armv7l",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rm -rf dist build node_modules/.cache",
    "clean:full": "rm -rf dist build node_modules",
    "bloat-check": "./scripts/bloat-check.sh",
    "temp-cleanup": "./scripts/temp-cleanup.sh",
    "build-clean": "./scripts/build-compile-dist.sh --skip-temp-clean --skip-bloat",
    "security-check": "./scripts/security-validation.sh",
    "quality-check": "./scripts/code-quality-validation.sh",
    "test-validation": "./scripts/testing-coverage-validation.sh",
    "performance-check": "./scripts/performance-analysis.sh",
    "repo-cleanup": "./scripts/repository-cleanup.sh",
    "final-verify": "./scripts/final-verification.sh",
    "validate-all": "./scripts/master-build-validation.sh --all",
    "validate-quick": "./scripts/master-build-validation.sh --quick",
    "validate-production": "./scripts/master-build-validation.sh",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/material": "^5.16.14",
    "@mui/x-date-pickers": "^6.20.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "date-fns": "^3.6.0",
    "electron-log": "^5.2.4",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.9",
    "lodash": "^4.17.21",
    "node-cron": "^4.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.30.1",
    "sqlite3": "^5.1.7",
    "xml2js": "^0.6.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.7.0",
    "@electron-forge/plugin-fuses": "^7.7.0",
    "@electron/fuses": "^1.8.0",
    "@testing-library/jest-dom": "^6.7.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/node": "^24.9.2",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "wait-on": "^8.0.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/main.ts"
    ]
  },
  "build": {
    "appId": "com.aifeed.app",
    "productName": "AIFEED",
    "copyright": "Copyright Â© 2024 Jasonn Michaels",
    "directories": {
      "output": "dist",
      "buildResources": "build-resources"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "!node_modules/.cache",
      "!node_modules/.bin",
      "!**/*.{md,txt,log}",
      "!**/*.map",
      "!node_modules/**/*.md",
      "!node_modules/**/*.txt",
      "!node_modules/**/test/**",
      "!node_modules/**/tests/**",
      "!node_modules/**/example*",
      "!node_modules/**/examples/**",
      "!node_modules/**/*.d.ts",
      "!node_modules/**/.github/**",
      "!node_modules/**/coverage/**"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "build-resources/icons/icon.icns",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "forceCodeSigning": false,
      "identity": null,
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "icon": "build-resources/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "build-resources/icons/icon.ico",
      "uninstallerIcon": "build-resources/icons/icon.ico",
      "installerHeaderIcon": "build-resources/icons/icon.ico",
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "AIFEED"
    },
    "msi": {
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "upgradeCode": "A1B2C3D4-E5F6-G7H8-I9J0-K1L2M3N4O5P6"
    },
    "linux": {
      "icon": "build-resources/icons",
      "category": "Utility",
      "executableName": "aifeed",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "snap",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "deb": {
      "depends": [
        "libgtk-3-0",
        "libnotify4",
        "libnss3",
        "libxss1",
        "libxtst6",
        "xdg-utils",
        "libatspi2.0-0",
        "libuuid1",
        "libsecret-1-0"
      ]
    },
    "rpm": {
      "depends": [
        "gtk3",
        "libnotify",
        "nss",
        "libXScrnSaver",
        "libXtst",
        "xdg-utils",
        "at-spi2-core",
        "libuuid",
        "libsecret"
      ]
    },
    "snap": {
      "grade": "stable",
      "summary": "AI Intelligence Dashboard"
    }
  }
}
